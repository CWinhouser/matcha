{{#each people as | person |}}
        <div class="row">
            <div class="col-md-3">
              <img src="/static/userImage/{{image}}" width=128px />
            </div>
            <div class="col-md-9">
                <div class="row">
                    <div class="col-md-4">
                        <span>{{ fullname }}</span>
                    </div>
                    <div class="col-md-4">
                        <span>Popularity: {{ popularity }}</span>
                    </div>
                    <div class="col-md-2">
                        <span>Age: {{ age }}</span>
                    </div>
                </div>
                <br />
                <div class="row">
                    <span>Likes</span>
                    <br />
                    <ul>
                        {{#each likes as | like |}}
                        <li>{{interest}}</li>
                        {{/each}}
                    </ul>
                </div>
            </div>
            <hr />
        </div>
        {{/each}}